{
  "name": "hotmesh-ui",
  "version": "0.0.5",
  "description": "HotMesh UI",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "tsc --build tsconfig.json",
    "clean-build": "npm run clean && npm run build",
    "server": "HMSH_LOGLEVEL='info' ts-node-dev --exit-child --respawn --transpile-only --ignore-watch node_modules --watch .env -- web/server.ts",
    "docker:up": "npm run _copy_env_example; npm run _docker:build:up",
    "docker:down": "docker compose down",
    "docker:reset": "docker compose down -v && npm run docker:up",
    "_docker:build:up": "docker compose watch",
    "_copy_env_example": "cp -n env.example .env",
    "test:hotmesh": "node ./test/hotmesh/index.js",
    "test:meshflow": "node ./test/meshflow/index.js",
    "test:meshdata": "node ./test/meshdata/index.js",
    "test:meshcall": "node ./test/meshcall/index.js",
    "test": " HMSH_SCOUT_INTERVAL_SECONDS=1 npm run test:hotmesh && npm run test:meshflow && npm run test:meshdata && npm run test:meshcall"
  },
  "keywords": [
    "Redis",
    "Postgres",
    "HotMesh",
    "Temporalio",
    "Durable Workflow",
    "Distributed Orchestration",
    "Operational Data Layer"
  ],
  "author": "luke.birdeau@gmail.com",
  "license": "Apache-2.0",
  "dependencies": {
    "@honeycombio/opentelemetry-node": "^0.7.2",
    "@hotmeshio/dashboard": "^0.3.7",
    "@hotmeshio/hotmesh": "^0.3.32",
    "@opentelemetry/auto-instrumentations-node": "^0.47.1",
    "@opentelemetry/sdk-node": "^0.52.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-winston": "^4.2.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "nats": "^2.28.0",
    "openai": "^4.29.2",
    "pg": "^8.10.0",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/connect": "^3.4.38",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
